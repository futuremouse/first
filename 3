import UIKit

func solution(_ num1:Int, _ num2:Int) -> Int {
    if (0 < num1 && num1 <= 100 && 1 < num2 && num2 <= 100 && num2 != 0) {
        return num1 / num2
    }
    return 0
}
let example1 = (num1: 10, num2: 5)
let example2 = (num1 : 7, num2: 2)

print("num1\tnum2\tresult")
print("\(example1.num1)\t\(example1.num2)\t\(solution(example1.num1,example1.num2))")
print("\(example2.num1)\t\(example2.num2)\t\(solution(example2.num1,example2.num2))")
