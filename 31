func solution(_ n:Int) -> String {
  // 결과값을 초기화 먼저
  var result = ""
  //"수박" 중 "수"로 시작해야하므로 1부터 입력("수로 시작") 값 n까지 반복문으로 구현
    for i in 1...n {
        if i % 2 == 1 {     //홀수
            result += "수"
        } else {            //짝수
            result += "박"
        }
    }
    return result
}

//예시
let n = 7 
print(solution(n)) // "수박수박수박수" 로 출력
